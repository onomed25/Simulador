{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/public/$1.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)\\.html",
      "destination": "/$1",
      "permanent": true
    },
    {
      "source": "/",
      "destination": "/index.html",
      "permanent": true
    }
  ]
}
